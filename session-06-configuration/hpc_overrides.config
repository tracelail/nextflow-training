// Custom configuration overrides for HPC execution
// Use with: nextflow run analysis_pipeline.nf -profile slurm -c hpc_overrides.config

process {
    withName: ANALYZE {
        cpus = 4
        memory = '8.GB'
        clusterOptions = '--account=special_project --qos=priority'
    }
}

params {
    outdir = 'hpc_results'
}
